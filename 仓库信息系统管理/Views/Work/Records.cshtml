@model List<Models.Ext.RecordsExt>

@{
    ViewBag.Title = "产品历史出入库详细记录";
    Layout = "~/Views/Shared/_LayoutPage_product.cshtml";
}

<div class="show_title"><p>产品历史出入库详细记录</p></div>
<div style="position:relative;bottom:70px;">@Html.Action("AdminDemo", "Login")</div>
<Div class="right_box">
    <div class="right_topBar">
        <ul>
            <li><a href="#">增加记录</a></li>
            <li><a href="#">导出记录</a></li>
        </ul>
    </div>

    <div class="content_box">
        <div>

        </div>
        <div class="content_all">
            <table width="95%" border="0" cellpadding="0" cellspacing="1" bgcolor="#999999">
                <tr>
                    <td align="center" valign="middle" bgcolor="#d1d1d1">记录流水号</td>
                    <td align="center" valign="middle" bgcolor="#d1d1d1">产品编号</td>
                    <td align="center" valign="middle" bgcolor="#d1d1d1">产品名称</td>
                    <td align="center" valign="middle" bgcolor="#d1d1d1">客户名称</td>
                    <td align="center" valign="middle" bgcolor="#d1d1d1">库存数量</td>
                    <td align="center" valign="middle" bgcolor="#d1d1d1">月计划数</td>
                    <td height="35" align="center" valig="middle" bgcolor="#d1d1d1">出库数</td>
                    <td height="35" align="center" valign="middle" bgcolor="#d1d1d1">入库数</td>
                    <td height="35" align="center" valig="middle" bgcolor="#d1d1d1">记录时间</td>
                    <td height="35" align="center" valign="middle" bgcolor="#d1d1d1">记录人</td>
                </tr>
                @{
                    foreach (var item in Model)
                    {
                        <tr>
                            <td align="center" valign="middle" bgcolor="#d1d1d1">@item.Id</td>
                            <td align="center" valign="middle" bgcolor="#d1d1d1">@item.ProductId</td>
                            <td align="center" valign="middle" bgcolor="#d1d1d1">@item.ProductName</td>
                            <td align="center" valign="middle" bgcolor="#d1d1d1">@item.Customer</td>
                            <td align="center" valign="middle" bgcolor="#d1d1d1">@item.Inventory</td>
                            <td align="center" valign="middle" bgcolor="#d1d1d1">@item.Plans</td>
                            <td height="35" align="center" valig="middle" bgcolor="#d1d1d1">@item.OutNum</td>
                            <td height="35" align="center" valign="middle" bgcolor="#d1d1d1">@item.InNum</td>
                            <td height="35" align="center" valig="middle" bgcolor="#d1d1d1">@item.Time</td>
                            <td height="35" align="center" valign="middle" bgcolor="#d1d1d1">@item.userName</td>
                        </tr>
                    }
                }
            </table>
            <div class="page">
                <ul>
                    <li><a href="#">1</a></li>
                    <li><a href="#">2</a></li>
                    <li><a href="#">3</a></li>
                    <li><a href="#">4</a></li>
                    <li><a href="#">5</a></li>
                </ul>

            </div>

        </div>
        <div class="clear"></div>
    </div>
</Div>